---
import 'src/styles/global.css';

import Layout from '@layouts/Layout.astro';
---

<Layout
  title="Surrendering a Sheltie"
  description="Information about SC Sheltie Rescue events."
>
  <div class="row">
    <div class="large-12 medium-12 small-12 columns">
      <main role="main" class="events main-content">
        <div class="border border-solid block font-normal my-5 p-3 relative bg-primary-c border-primary-c-darker text-zinc-100">
          <p class="mb-0"><strong>***Check back here for any scheduled events!***</strong></p>
        </div>
      </main>
    </div>
  </div>
</Layout>
